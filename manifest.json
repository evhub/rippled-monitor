{
    "manifest_version": 2,
    "name": "Rippled Monitor",
    "description": "Monitors A Local Rippled Instance.",
    "version": "0.0.1",
    "minimum_chrome_version": "33",
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "sockets": {
        "udp": {
            "receive": ""
        }
    },
    "permissions": {
        [{
        "socket": ["udp-receive"]
        }]
    }
}
